{
  "name": "@udus/notion-renderer",
  "version": "1.0.0-alpha.51",
  "type": "module",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/udus122/notion-renderer.git"
  },
  "keywords": [
    "notion",
    "react",
    "notion-api"
  ],
  "main": "dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./components": {
      "import": "./dist/components/index.js",
      "types": "./dist/components/index.d.ts"
    },
    "./libs": {
      "import": "./dist/libs/index.js",
      "types": "./dist/libs/index.d.ts"
    },
    "./types": {
      "import": "./dist/types/index.js",
      "types": "./dist/types/index.d.ts"
    },
    "./styles/*.css": {
      "import": "./dist/styles/*.css",
      "require": "./dist/styles/*.css"
    }
  },
  "typesVersions": {
    "*": {
      "components": [
        "dist/components/index.d.ts"
      ],
      "libs": [
        "dist/libs/index.d.ts"
      ]
    }
  },
  "files": [
    "/dist"
  ],
  "scripts": {
    "clean": "rimraf dist/*",
    "prebuild": "npm run clean",
    "build": "run-p build:*",
    "build:ts": "tsc",
    "build:css": "postcss src/styles/globals.css -o dist/styles/globals.css",
    "build:sb": "storybook build && touch storybook-static/.nojekyll",
    "watch": "run-p watch:*",
    "watch:ts": "tsc -w",
    "watch:css": "postcss src/styles/globals.css -o dist/styles/globals.css -w",
    "lint": "run-p lint:*",
    "lint:js": "run-p lint:eslint lint:prettier lint:tsc",
    "lint:eslint": "eslint src --ext .js,.jsx,.ts,.tx",
    "lint:prettier": "prettier --check src",
    "lint:tsc": "tsc --noEmit",
    "lint:css": ":",
    "fix": "run-p fix:*",
    "fix:eslint": "eslint src --ext .js,.jsx,.ts,.tx --fix",
    "fix:prettier": "prettier --write src",
    "prepare": "husky install",
    "sb": "storybook dev -p 6006 --no-open",
    "semantic-release": "semantic-release",
    "dev": "vite --open",
    "preview": "vite preview",
    "test": "exit 1"
  },
  "peerDependencies": {
    "katex": "^0.16.9",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "dependencies": {
    "@extractus/article-extractor": "^8.0.4",
    "@extractus/oembed-extractor": "^4.0.1",
    "@notionhq/client": "^2.2.14",
    "date-fns": "^2.30.0",
    "date-fns-tz": "^2.0.0",
    "dotenv": "^16.3.1",
    "modern-normalize": "^2.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@semantic-release/git": "^10.0.1",
    "@storybook/addon-console": "^2.0.0",
    "@storybook/addon-essentials": "^7.6.4",
    "@storybook/addon-interactions": "^7.6.4",
    "@storybook/addon-links": "^7.6.4",
    "@storybook/addon-mdx-gfm": "^7.6.4",
    "@storybook/blocks": "^7.6.4",
    "@storybook/react": "^7.6.4",
    "@storybook/react-vite": "^7.6.4",
    "@storybook/testing-library": "^0.2.2",
    "@types/katex": "^0.16.7",
    "@types/node": "^20.10.4",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "autoprefixer": "^10.4.16",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "husky": "^8.0.3",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^11.0.0",
    "postcss-import": "^15.1.0",
    "postcss-nesting": "^12.0.1",
    "prettier": "^3.1.0",
    "prop-types": "^15.8.1",
    "rimraf": "^5.0.5",
    "semantic-release": "^22.0.10",
    "storybook": "^7.6.4",
    "stylelint": "^16.0.1",
    "stylelint-config-standard": "^35.0.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.7",
    "vite-plugin-node-polyfills": "^0.17.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
