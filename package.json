{
  "name": "@udus/notion-renderer",
  "version": "1.0.0-alpha.86",
  "type": "module",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/udus122/notion-renderer.git"
  },
  "keywords": [
    "notion",
    "react",
    "notion-api"
  ],
  "packageManager": "npm@10.9.2",
  "main": "dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./components": {
      "import": "./dist/components/index.js",
      "types": "./dist/components/index.d.ts"
    },
    "./libs": {
      "import": "./dist/libs/index.js",
      "types": "./dist/libs/index.d.ts"
    },
    "./utils": {
      "import": "./dist/utils/index.js",
      "types": "./dist/utils/index.d.ts"
    },
    "./types": {
      "import": "./dist/types/index.js",
      "types": "./dist/types/index.d.ts"
    },
    "./styles/*.css": {
      "import": "./dist/styles/*.css",
      "require": "./dist/styles/*.css"
    }
  },
  "typesVersions": {
    "*": {
      "components": [
        "dist/components/index.d.ts"
      ],
      "libs": [
        "dist/libs/index.d.ts"
      ],
      "utils": [
        "dist/utils/index.d.ts"
      ],
      "types": [
        "dist/types/index.d.ts"
      ]
    }
  },
  "files": [
    "/dist"
  ],
  "scripts": {
    "clean": "rimraf dist/*",
    "prebuild": "npm run clean",
    "build": "run-p build:*",
    "build:ts": "tsc",
    "build:css": "postcss src/styles/globals.css -o dist/styles/globals.css",
    "build:sb": "storybook build && touch storybook-static/.nojekyll",
    "watch": "run-p watch:*",
    "watch:ts": "tsc -w",
    "watch:css": "postcss src/styles/globals.css -o dist/styles/globals.css -w",
    "lint": "run-p lint:*",
    "lint:js": "run-p lint:biome lint:tsc",
    "lint:biome": "biome check src",
    "lint:tsc": "tsc --noEmit",
    "fix": "run-p fix:*",
    "fix:biome": "biome format src --write",
    "prepare": "husky",
    "sb": "storybook dev -p 6006 --no-open",
    "semantic-release": "semantic-release",
    "dev": "vite --open",
    "preview": "vite preview",
    "test": "exit 1"
  },
  "peerDependencies": {
    "katex": "^0.16.9",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "dependencies": {
    "@extractus/article-extractor": "^8.0.17",
    "@extractus/oembed-extractor": "^4.0.6",
    "@notionhq/client": "^2.2.15",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "dotenv": "^16.4.7",
    "modern-normalize": "^3.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.7.1",
    "@commitlint/config-conventional": "^19.7.1",
    "@semantic-release/git": "^10.0.1",
    "@storybook/addon-console": "^3.0.0",
    "@storybook/addon-essentials": "^8.5.6",
    "@storybook/addon-interactions": "^8.5.6",
    "@storybook/addon-links": "^8.5.6",
    "@storybook/addon-mdx-gfm": "^8.5.6",
    "@storybook/blocks": "^8.5.6",
    "@storybook/react": "^8.5.6",
    "@storybook/react-vite": "^8.5.6",
    "@storybook/testing-library": "^0.2.2",
    "@types/katex": "^0.16.7",
    "@types/node": "^22.13.4",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@vitejs/plugin-react-swc": "^3.8.0",
    "autoprefixer": "^10.4.20",
    "biome": "^0.2.2",
    "cz-conventional-changelog": "3.3.0",
    "husky": "^9.1.7",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^11.0.0",
    "postcss-import": "^16.1.0",
    "postcss-nesting": "^13.0.1",
    "prop-types": "^15.8.1",
    "rimraf": "^6.0.1",
    "semantic-release": "^24.2.3",
    "storybook": "^8.5.6",
    "stylelint": "^16.14.1",
    "stylelint-config-standard": "^37.0.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "turbo": "^2.4.2",
    "typescript": "^5.7.3",
    "vite": "^6.1.0",
    "vite-plugin-node-polyfills": "^0.23.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
