{
  "name": "notion-component",
  "version": "1.0.0-alpha.4",
  "type": "module",
  "license": "MIT",
  "main": "./dist/index.umd.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "/dist"
  ],
  "scripts": {
    "dev": "vite --open",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "",
    "lint:js": "eslint src --ext .jsx,.js,.tsx,.ts",
    "lint:css": ":",
    "check": "tsc && prettier --list-different . && npm run lint:js && npm run lint:css && npm run test",
    "format": "prettier --write . && npm run lint:js -- --fix && npm run lint:css -- --fix",
    "prepare": "husky install",
    "semantic-release": "semantic-release",
    "sb": "storybook dev -p 6006",
    "build-sb": "storybook build",
    "example": "npm run build && (cd example && npm run example)"
  },
  "dependencies": {
    "@notionhq/client": "^2.2.3",
    "clsx": "^1.2.1",
    "modern-normalize": "^1.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-katex": "^3.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.5.1",
    "@commitlint/config-conventional": "^17.4.4",
    "@semantic-release/git": "^10.0.1",
    "@storybook/addon-essentials": "^7.4.5",
    "@storybook/addon-interactions": "^7.4.5",
    "@storybook/addon-links": "^7.4.5",
    "@storybook/addon-mdx-gfm": "^7.4.5",
    "@storybook/blocks": "^7.4.5",
    "@storybook/react": "^7.4.5",
    "@storybook/react-vite": "^7.4.5",
    "@storybook/testing-library": "^0.2.1",
    "@types/node": "^18.15.11",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/react-katex": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "@vitejs/plugin-react-swc": "^3.0.0",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "^8.37.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.4",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^8.0.0",
    "prettier": "^2.8.7",
    "prop-types": "^15.8.1",
    "semantic-release": "^21.0.1",
    "storybook": "^7.4.5",
    "typescript": "^5.0.4",
    "vite": "^4.2.0",
    "vite-plugin-dts": "^2.1.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/udus122/notion-component.git"
  },
  "publishConfig": {
    "access": "public"
  }
}
