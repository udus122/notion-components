{
  "name": "notion-component",
  "version": "1.0.0-alpha.4",
  "type": "module",
  "license": "MIT",
  "main": "./dist/index.umd.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "/dist"
  ],
  "scripts": {
    "dev": "vite --open",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "lint:js": "eslint src --ext .jsx,.js,.tsx,.ts",
    "lint:css": ":",
    "check": "tsc && prettier --list-different . && npm run lint:js && npm run lint:css && npm run test",
    "format": "prettier --write . && npm run lint:js -- --fix && npm run lint:css -- --fix",
    "prepare": "husky install",
    "semantic-release": "semantic-release",
    "sb": "storybook dev -p 6006",
    "build-sb": "storybook build",
    "example": "npm run build && (cd example && npm run example)"
  },
  "dependencies": {
    "@notionhq/client": "^2.2.13",
    "modern-normalize": "^1.1.0",
    "notionate": "^9.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-katex": "^3.0.1",
    "react-syntax-highlighter": "^15.5.0",
    "react-youtube": "^10.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.2",
    "@commitlint/config-conventional": "^17.7.0",
    "@semantic-release/git": "^10.0.1",
    "@storybook/addon-console": "^2.0.0",
    "@storybook/addon-essentials": "^7.4.5",
    "@storybook/addon-interactions": "^7.4.5",
    "@storybook/addon-links": "^7.4.5",
    "@storybook/addon-mdx-gfm": "^7.4.5",
    "@storybook/blocks": "^7.4.5",
    "@storybook/react": "^7.4.5",
    "@storybook/react-vite": "^7.4.5",
    "@storybook/testing-library": "^0.2.1",
    "@types/node": "^18.18.0",
    "@types/react": "^18.2.23",
    "@types/react-dom": "^18.2.8",
    "@types/react-katex": "^3.0.1",
    "@types/react-syntax-highlighter": "^15.5.7",
    "@types/youtube-player": "^5.5.8",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vitejs/plugin-react-swc": "^3.4.0",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^8.0.3",
    "prettier": "^2.8.8",
    "prop-types": "^15.8.1",
    "semantic-release": "^21.1.2",
    "storybook": "^7.4.5",
    "typescript": "^5.2.2",
    "vite": "^4.4.9",
    "vite-plugin-dts": "^2.3.0",
    "vitest": "^0.34.6"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/udus122/notion-component.git"
  },
  "publishConfig": {
    "access": "public"
  }
}